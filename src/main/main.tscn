script/source = "extends LineEdit

var command_index: int = -1


func _input(event: InputEvent) -> void:
	if event.is_action_pressed(\"ui_up\"):
		if command_index > Console.command_history.size():
			command_index += 1
			text = Console.command_history[command_index]
		print(command_index)
		print(Console.command_history)



func _on_line_edit_text_submitted(new_text: String) -> void:
	text = \"\"
	
	var split_string: PackedStringArray = new_text.split(\" \")
	var command: String = split_string[0]
	var args = split_string.remove_at(0)
	Console.execute_command(command, args)
	command_index = -1
"

